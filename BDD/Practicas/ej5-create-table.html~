<pre class="src src-sql"><span class="org-keyword">CREATE</span> <span class="org-keyword">TABLE</span> <span class="org-function-name">agenda</span> (
  c_amigo <span class="org-keyword">NUMBER</span>(4) <span class="org-keyword">NOT</span> <span class="org-keyword">NULL</span>,
  nombre VARCHAR2(12),
  apellidos VARCHAR2(20),
  f_nac <span class="org-type">DATE</span>,
  telefono <span class="org-keyword">NUMBER</span>(7),
  <span class="org-keyword">CONSTRAINT</span> pk_agenda <span class="org-keyword">PRIMARY</span> <span class="org-keyword">KEY</span>(c_amigo)
);

<span class="org-keyword">INSERT</span> <span class="org-keyword">INTO</span> "VIDEO"."AGENDA" (
  c_amigo,
  nombre,
  apellidos,
  f_nac,
  telefono
) <span class="org-keyword">VALUES</span> (
  <span class="org-string">'1'</span>,
  <span class="org-string">'Pepino'</span>,
  <span class="org-string">'Pimiento'</span>,
  TO_DATE(<span class="org-string">'01/01/01'</span>, <span class="org-string">'DD/MM/RR'</span>),
  <span class="org-string">'1111111'</span>
);

<span class="org-keyword">INSERT</span> <span class="org-keyword">INTO</span> "VIDEO"."AGENDA" (
  c_amigo,
  nombre,
  apellidos,
  f_nac,
  telefono
) <span class="org-keyword">VALUES</span> (
  <span class="org-string">'2'</span>,
  <span class="org-string">'Cebolla'</span>,
  <span class="org-string">'Comino'</span>,
  TO_DATE(<span class="org-string">'02/02/02'</span>, <span class="org-string">'DD/MM/RR'</span>),
  <span class="org-string">'2222222'</span>
);

<span class="org-keyword">SELECT</span> *
<span class="org-keyword">FROM</span>   agenda;
</pre>
